import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int t = input.nextInt();
        while (t-- !=0 ) {
            String s = input.next();
            if (s.equalsIgnoreCase("yes")) {
                System.out.println("YES");
            }
            else
            System.out.println("NO");
        }
    }
}
