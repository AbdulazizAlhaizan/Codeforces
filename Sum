import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int t = input.nextInt();
        while (t-- !=0 ) {
            int a = input.nextInt();
            int b = input.nextInt();
            int c = input.nextInt();
            if (a + b == c || a + c == b ||c + b == a )
            System.out.println("YES");
            else
            System.out.println("NO");
        }
    }
}
